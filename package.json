{
  "name": "antd-spin",
  "version": "1.0.4",
  "license": "MIT",
  "author": "Condor Hero",
  "description": "Show animation while loading data.",
  "bin": {},
  "main": "lib/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "antd",
    "loading",
    "spin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/condorheroblog/antd-spin.git"
  },
  "bugs": {
    "url": "https://github.com/condorheroblog/antd-spin/issues"
  },
  "homepage": "https://github.com/condorheroblog/antd-spin/blob/main/README.md",
  "scripts": {
    "build": "babel src -d lib",
    "build:rollup": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "format": "prettier --write 'src/**/*.{js,scss,css}'",
    "lint": "eslint ./src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint -c .eslintrc"
    ],
    "*.ts": [
      "prettier --parser=typescript --write",
      "eslint -c .eslintrc"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.1",
    "antd": "^4.8.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.33.3",
    "rollup-plugin-postcss": "^3.1.8"
  }
}
