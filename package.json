{
	"name": "antd-spin",
	"version": "1.0.6",
	"type": "module",
	"license": "MIT",
	"author": "Condor Hero",
	"description": "Enhancing Ant Design's Spin as a Service.",
	"bin": {},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"keywords": [
		"antd-service-spin",
		"antd-spin",
		"loading",
		"spin"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/antd-spin.git"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/antd-spin/issues"
	},
	"homepage": "https://github.com/condorheroblog/antd-spin/blob/main/README.md",
	"scripts": {
		"prepublishOnly": "nr build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"test": "vitest",
		"build:rollup": "rimraf dist && rollup -c",
		"rollup:watch": "rimraf dist && rollup -c -w",
		"typecheck": "tsc --noEmit",
		"release": "bumpp -r && npm publish"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{ts,js,jsx,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"peerDependencies": {
		"antd": ">=4",
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@types/node": "^20.4.7",
		"@types/react": "^18.2.18",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"bumpp": "^9.1.1",
		"esbuild": "^0.18.17",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.1",
		"less": "^4.2.0",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.0",
		"rimraf": "^5.0.1",
		"rollup": "^3.27.0",
		"rollup-plugin-dts": "^5.3.1",
		"rollup-plugin-esbuild": "^5.0.0",
		"rollup-plugin-postcss": "^3.1.8",
		"simple-git-hooks": "^2.9.0",
		"typescript": "^5.1.6",
		"vitest": "^0.34.1"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
